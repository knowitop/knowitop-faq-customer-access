<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
  <constants>
  </constants>
  <classes>
    <class id="FAQ">
      <fields>
        <field id="status" xsi:type="AttributeEnum" _delta="define_if_not_exists">
          <values>
            <value id="draft">draft</value>
            <value id="published">published</value>
            <value id="obsolete">obsolete</value>
          </values>
          <sql>status</sql>
          <default_value>draft</default_value>
          <is_null_allowed>false</is_null_allowed>
          <display_style>list</display_style>
        </field>
        <field id="customers_list" xsi:type="AttributeLinkedSetIndirect" _delta="define">
          <linked_class>lnkFAQToCustomer</linked_class>
          <ext_key_to_me>faq_id</ext_key_to_me>
          <count_min>0</count_min>
          <count_max>0</count_max>
          <ext_key_to_remote>customer_id</ext_key_to_remote>
          <duplicates/>
        </field>
      </fields>
      <methods/>
      <presentation>
        <details>
          <items>
            <item id="status" _delta="define_if_not_exists">
              <rank>25</rank>
            </item>
            <item id="customers_list" _delta="define">
              <rank>100</rank>
            </item>
          </items>
        </details>
        <search>
          <items>
            <item id="status" _delta="define_if_not_exists">
              <rank>15</rank>
            </item>
          </items>
        </search>
        <default_search>
          <items>
            <item id="status" _delta="define_if_not_exists">
              <rank>25</rank>
            </item>
          </items>
        </default_search>
        <list>
          <items>
            <item id="status" _delta="define_if_not_exists">
              <rank>15</rank>
            </item>
          </items>
        </list>
      </presentation>
    </class>
    <class id="lnkFAQToCustomer" _delta="define">
      <parent>cmdbAbstractObject</parent>
      <properties>
        <is_link>1</is_link>
        <category>bizmodel</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>lnkfaqtocustomer</db_table>
        <db_key_field>id</db_key_field>
        <db_final_class_field/>
        <naming>
          <attributes>
            <attribute id="faq_id"/>
            <attribute id="customer_id"/>
          </attributes>
        </naming>
        <display_template/>
        <icon/>
        <reconciliation>
          <attributes>
            <attribute id="faq_id"/>
            <attribute id="customer_id"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
        <field id="faq_id" xsi:type="AttributeExternalKey">
          <sql>faq_id</sql>
          <target_class>FAQ</target_class>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
        </field>
        <field id="faq_title" xsi:type="AttributeExternalField">
          <extkey_attcode>faq_id</extkey_attcode>
          <target_attcode>title</target_attcode>
        </field>
        <field id="customer_id" xsi:type="AttributeExternalKey">
          <sql>customer_id</sql>
          <target_class>Organization</target_class>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_AUTO</on_target_delete>
        </field>
        <field id="customer_name" xsi:type="AttributeExternalField">
          <extkey_attcode>customer_id</extkey_attcode>
          <target_attcode>name</target_attcode>
        </field>
      </fields>
      <methods/>
      <presentation>
        <details>
          <items>
            <item id="faq_id">
              <rank>10</rank>
            </item>
            <item id="customer_id">
              <rank>20</rank>
            </item>
          </items>
        </details>
        <search>
          <items>
            <item id="faq_id">
              <rank>10</rank>
            </item>
            <item id="customer_id">
              <rank>20</rank>
            </item>
          </items>
        </search>
        <list>
          <items>
            <item id="faq_id">
              <rank>10</rank>
            </item>
            <item id="customer_id">
              <rank>20</rank>
            </item>
          </items>
        </list>
      </presentation>
    </class>
  </classes>
  <menus>
  </menus>
  <user_rights>
    <groups>
      <group id="KnownError">
        <classes>
          <class id="lnkFAQToCustomer" _delta="define"/>
        </classes>
      </group>
    </groups>
    <profiles>
    </profiles>
  </user_rights>
  <module_designs>
    <module_design id="itop-portal" _delta="must_exist">
      <classes>
        <class id="FAQ">
          <scopes>
            <scope id="all" _delta="redefine">
              <oql_view><![CDATA[SELECT FAQ AS faq JOIN lnkFAQToCustomer AS lnk ON lnk.faq_id = faq.id JOIN Organization AS root ON lnk.customer_id = root.id JOIN Organization AS node ON node.parent_id BELOW root.id WHERE node.id = :current_contact->org_id AND faq.status = 'published']]></oql_view>
            </scope>
          </scopes>
        </class>
      </classes>
    </module_design>
  </module_designs>
</itop_design>
